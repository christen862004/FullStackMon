@model Employee


<div class="card" style="width:300px">
    <img class="card-img-top" src="/Images/@Model.ImageUrl" alt="Card image" style="width:100px">
    <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <p class="card-text">@Model.Address</p>
        <a href="/Employee/Edit/@Model.Id" class="btn btn-primary">Edit</a>
    </div>
</div>
