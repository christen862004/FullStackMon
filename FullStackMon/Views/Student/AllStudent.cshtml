<!--View <== Model (List<student>)-->
<table class="table table-bordered table-hover">
@foreach (Student item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Address</td>
        <td>@item.Image</td>
        <td><img src="~/Images/@item.Image" style="width:100px" /></td>

    </tr>
}
</table>
